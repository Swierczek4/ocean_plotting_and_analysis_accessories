clear
close all
clc

tic()

acc_colors


X1 = 1:0.1:10;
X2 = 0.5:0.1:10;
lw = 6;
coords = [0 10 0 51];

figure()
set(gcf, 'Position', [1, 1, 1600, 900])
plot(X1,ones(91,1),'LineWidth',lw,'Color',Color(:,1))
hold on
for ii=2:50
    if mod(ii,2)==0
        plot(X2,ii*ones(96,1),'LineWidth',lw,'Color',Color(:,ii))
    else
        
    end
end

for ii=1:50
    text(0.5,ii,num2str(ii),'FontSize',13)
end
axis(coords)
ax.AmbientLightColor = Color(:,36)';
set(gca,'xtick',[])
set(gca,'ytick',[])
hold off





toc()